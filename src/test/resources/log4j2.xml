<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>
<configuration status="WARN">
  <appenders>
    <console name="console" target="SYSTEM_OUT">
      <patternLayout pattern="%d{HH:mm:ss.SSS} %-45logger{36}:%-4L - %m%n"/>
    </console>
  </appenders>
  <loggers>
    <root level="debug">
      <appenderRef ref="console"/>
    </root>
  </loggers>
</configuration>